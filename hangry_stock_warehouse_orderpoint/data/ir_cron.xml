<odoo>
  <data noupdate="1">
    <record id="ir_cron_stock_orderpoint_cleanup" model="ir.cron">
      <field name="name">[Perf] Cleanup Processed Orderpoints</field>
      <field name="model_id" ref="stock.model_stock_warehouse_orderpoint"/>
      <field name="state">code</field>
      <field name="code">model.cron_cleanup_processed_orderpoints()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">hours</field>
      <field name="nextcall" eval="(DateTime.now() + relativedelta(minutes=10)).strftime('%Y-%m-%d %H:%M:%S')"/>
      <field name="active">True</field>
      <field name="user_id" ref="base.user_root"/>
      <field name="priority">5</field>
    </record>
  </data>
</odoo>
